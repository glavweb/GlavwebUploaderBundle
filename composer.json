{
    "name": "glavweb/uploader-bundle",
    "description": "Symfony GlavwebUploaderBundle",
    "authors": [
        {
            "name": "Andrey Nilov",
            "email": "nilov@glavweb.ru",
            "homepage": "https://glavweb.ru"
